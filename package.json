{
    "author": "",
    "dependencies": {
        "@discordx/importer": "^1.3.1",
        "@types/node": "^22.10.1",
        "chalk": "^5.3.0",
        "discord.js": "^14.16.3",
        "discordx": "^11.12.2",
        "dotenv": "^16.4.7",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2"
    },
    "description": "Bombot is a JavaScript-based rewrite of\r [@18maherc](https://github.com/18maherc)'s\r [Discord-Bot](https://github.com/18maherc/Discord-Bot) project. It re-implements\r all features of the prior as well as adds new ones, and all runs on a Docker\r container hosted by [@AndrewJones-PSU](https://github.com/AndrewJones-PSU).",
    "devDependencies": {
        "@eslint/js": "^9.16.0",
        "@typescript-eslint/eslint-plugin": "^8.17.0",
        "@typescript-eslint/parser": "^8.17.0",
        "eslint": "^9.16.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.13.0",
        "nodemon": "^3.1.7",
        "prettier": "^3.4.2",
        "prettier-plugin-organize-imports": "^4.1.0",
        "typescript-eslint": "^8.17.0"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    },
    "license": "MIT",
    "main": "build/main.js",
    "name": "bombot",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf build",
        "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
        "format": "eslint . --fix && npx prettier . --write",
        "lint": "eslint .",
        "start": "npm run lint && npm run build && node build/main.js",
        "test": "nodemon --exec npm run dev --watch src --ext ts"
    },
    "type": "module",
    "version": "1.0.0"
}
